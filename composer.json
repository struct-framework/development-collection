{
  "name": "struct/development-collection",
  "type": "development",
  "description": "",
  "keywords": ["framework"],
  "license": "MIT",
  "authors": [
    {
      "name": "Timo Poppinga",
      "email": "timo.poppinga@zdrei.com"
    }
  ],
  "require": {
    "php": "^8.2.0"
  },
  "replace": {
    "struct/struct": "self.version",
    "struct/struct-contracts": "self.version",
    "struct/data-type": "self.version",
    "struct/data-type-contracts": "self.version"
  },
  "require-dev": {
    "ext-simplexml": "*",
    "symfony/process": "^6.3",
    "phpstan/phpstan": "^1.10.28",
    "phpstan/phpstan-strict-rules": "^1.5.1",
    "thecodingmachine/phpstan-strict-rules": "^v1.0.0",
    "friendsofphp/php-cs-fixer": "^v3.22.0",
    "phpunit/phpunit": "^10.3.1"
  },
  "autoload": {
    "psr-4": {
      "Struct\\Attribute\\": "packages/contracts/src-attribute",
      "Struct\\Contracts\\": "packages/contracts/src-contracts",
      "Struct\\Enum\\": "packages/contracts/src-enum",
      "Struct\\Exception\\": "packages/contracts/src-exception",

      "Struct\\Serializer\\": "packages/serializer/src",
      "Struct\\Serializer\\Private\\": "packages/serializer/src-private",

      "Struct\\Struct\\": "packages/struct/src",
      "Struct\\Struct\\Private\\": "packages/struct/src-private",

      "Struct\\DataType\\": "packages/data-type/src",
      "Struct\\DataType\\Private\\": "packages/data-type/src-private",

      "Struct\\Operator\\": "packages/operator/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Struct\\Development\\": "src",
      "Struct\\TestData\\": "tests/test-data",
      "Struct\\Operator\\Tests\\": "tests/operator",
      "Struct\\Serializer\\Tests\\": "tests/serializer",
      "Struct\\Struct\\Tests\\": "tests/struct",
      "Struct\\DataType\\Tests\\": "tests/data-type"
    }
  },
  "bin": [
    "binary/console"
  ],
  "config": {
    "bin-dir": "bin"
  }
}
